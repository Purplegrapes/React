{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    ["import",
      {
        "libraryName": "antd",
        "libraryDirectory": "lib",
        "style": true
      },
      {
        "libraryName": "antd-mobile",
        "libraryDirectory": "component"
      }

    ]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  }
}
